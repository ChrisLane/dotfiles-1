[user]
	email = mishajw@gmail.com
	name = Misha Wagner
[push]
	default = current
[core]
	editor = vim
[alias]
  a = add
  ap = add --patch
  b = branch
  c = commit --message
  ca = commit --all --message
  cam = commit --amend
  co = checkout
  d = diff
  f = fetch
  l = log --oneline --decorate --graph
  m = merge
  p = push
  pf = push --force
  pl = pull
  psup = push --set-upstream origin $(git_current_branch)
  rb = rebase
  rbi = rebase -i
  rbih = rebase -i HEAD~20
  s = status
  si = submodule init
  st = stash
  std = stash drop
  stp = stash pop
  su = submodule update

